{

  "name": "Clinterson",
  "description": "This extension will replace pictures of Hilary Clinton with Clinterson.",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab"
   ],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["popup.js"]
  }],

  "manifest_version": 2
}